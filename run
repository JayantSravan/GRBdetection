#!/bin/bash
rm signals -r
mkdir signals
k=150
j=0
i=50
while [ $i -lt 150 ];do
	./datagen.out l $i $k
	cat data.txt>signals/$j.txt
	let j=j+1
	let i=i+1 
done

i=50
while [ $i -lt 200 ];do
	./datagen.out g $i $k
	cat data.txt>signals/$j.txt
	let j=j+1
	let i=i+1 
done

i=150
while [ $i -lt 350 ];do
	./datagen.out t $i $k
	cat data.txt>signals/$j.txt
	let j=j+1
	let i=i+1 
done

i=50
while [ $i -lt 250 ];do
	./datagen.out f $i $k
	cat data.txt>signals/$j.txt
	let j=j+1
	let i=i+1 
done

i=70
while [ $i -lt 150 ];do
	./datagen.out e $i $k
	cat data.txt>signals/$j.txt
	let j=j+1
	let i=i+1 
done

let k=k+20







